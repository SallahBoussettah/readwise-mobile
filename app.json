{
  "expo": {
    "name": "ReadWise+",
    "slug": "readwise-plus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ReadWiseIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/ReadWiseIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.readwiseplus.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.readwiseplus.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/ReadWiseIcon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/ReadWiseIcon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c993e929-2c9e-4da0-bdb6-51775f666641"
      }
    }
  }
}
